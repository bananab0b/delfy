<div class="container">
  <div class="questionnaires_<%= action_name %>">
    <h1>
      
    </h1>
    <hr>
    <% if @surveys.empty? %>
      <p> There are no surveys </p>
    <% else %>
      <p>
      </p>
      <table class='table table-nonfluid survey-table'>
        <thead>
          <tr>
            <th></th>
            <th>Name</th>
            <th>Beschreibung</th>
           
            <th>Teilnehmer</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <%= render partial: 'survey', collection: @surveys %>
        </tbody>
      </table>
      <%= paginate @surveys %>
    <% end %>
  </div>
</div>
